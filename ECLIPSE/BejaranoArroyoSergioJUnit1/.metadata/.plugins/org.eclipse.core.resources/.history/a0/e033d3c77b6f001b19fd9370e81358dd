import junit.framework.TestCase;

public class RestaTest extends TestCase {
	/*
	 * @Test // Fallido public void testdecrementa() { Resta resta = new Resta();
	 * double resultado = resta.decrementa(2.6); assertEquals(1.4, resultado); }
	 * 
	 * @Test // Fallido public void getDiferencia() { Resta resta = new Resta();
	 * double resultado = resta.getDiferencia(2.0, 1.0); assertEquals(1.5,
	 * resultado); }
	 */

	@Test // Correcto
	public void testdecrementa() {
		Resta resta = new Resta();
		double resultado = resta.decrementa(2.6);
		assertEquals(1.6, resultado);
	}

	@Test // Correcto
	public void getDiferencia() {
		Resta resta = new Resta();
		double resultado = resta.getDiferencia(2.0, 1.0);
		assertEquals(1.0, resultado);
	}
}